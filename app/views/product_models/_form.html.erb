<%= form_with model: @product_model do |form| %> 
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :weight %>
    <%= form.number_field :weight %>
  </div>

  <div>
    <%= form.label :width %>
    <%= form.number_field :width %>
  </div>

  <div>
    <%= form.label :height %>
    <%= form.number_field :height %>
  </div>

  <div>
    <%= form.label :depth %>
    <%= form.number_field :depth %>
  </div>

  <div>
    <%= form.label :sku %>
    <%= form.text_field :sku %>
  </div>

  <div>
    <%= form.label :supplier_id %>
    <%= form.collection_select(:supplier_id, @suppliers, :id, :brand_name) %>
  </div>

  <div>
    <%= form.submit 'Enviar' %>
  </div>
<% end %>